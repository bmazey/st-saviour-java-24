package oop.Beverages;

public abstract class Drinks {

    private string Flavor;

    public string get Flavor(){
        return this.Flavor;
    }
    public void setFlavor (String Flavor){
        this.flavor = Flavor; 



        public String Drink (string) {
            string text = this.flavor + "smells like" + scent + "!";
            return text; 
        }
    }
}
